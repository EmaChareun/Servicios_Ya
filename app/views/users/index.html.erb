<div class= "container">
<h1> Profesionales </h1>

  <% @users_profesionals.each do |user| %>
    <div class="card-info">
          <h2><%= user.first_name %></h2>
          <p><%= user.job %></p>
          <p><%= user.city %></p>
          <br>
          <%= link_to 'mas detalles',  user_path(user), class: 'btn btn-primary' %>
    </div>
  <% end %>
</div>
